<!DOCTYPE html>
<html>
<head >
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body ng-app="myApp">
    @RenderBody()
    <script src="//code.jquery.com/jquery-2.2.4.min.js" ></script>
    <script type="text/javascript">window.jQuery || document.write('<script src="/Scripts/jquery-2.2.4.min.js"><\/script>')</script>
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular.min.js"></script>
    <script type="text/javascript">window.angular || document.write('<script src="/Scripts/angular.min.js"><\/script>')</script>
    <script type="text/javascript" src="https://code.angularjs.org/1.5.6/angular-route.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.6/angular-resource.min.js"></script>
    @Scripts.Render("~/bundles/angularApp")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", false)
</body>
</html>
